.text-up
  text-transform: uppercase
.text-center
  text-align: center

.no-br
  display: none
  +above($breakpoints.sm)
    display: block
.no-br-md
  display: none
  +above($breakpoints.md)
    display: block
.no-br-xxl
  display: none
  +above($breakpoints.xxl)
    display: block

.color-white
  color: $color-white
.color-black
  color: $color-black

.color-green
  color: $color-green

.gradient-text-stroke
  position: relative
  &:before
    content: attr(data-text)
    position: absolute
    top: 0
    left: 0
    z-index: -1
    color: transparent
    -webkit-text-stroke: 2px transparent
    background: linear-gradient(to bottom, rgba(224, 224, 224, 0), rgba(243, 243, 243, 1))
    -webkit-background-clip: text
    background-clip: text
    -webkit-text-stroke-color: rgba(0, 0, 0, 0.1)
    -webkit-text-fill-color: transparent

